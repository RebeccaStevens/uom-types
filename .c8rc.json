{
  "all": true,
  "include": ["src/**/*"],
  "exclude": ["src/*.ts", "src/units/*.ts", "src/units/[!converters]*/*.ts"],
  "reporter": ["lcov", "text"],
  "check-coverage": false,
  "watermarks": {
    "lines": [80, 95],
    "functions": [80, 95],
    "branches": [80, 95],
    "statements": [80, 95]
  }
}
